record(ao, "$(P)VOLT:SP") {
   field(DESC, "Sets the voltage")
   field(DTYP, "stream")
   field(EGU, "V")
   field(OUT, "@devWeeder.proto set_voltage($(ADDRESS)) $(PORT)")
   field(ASLO, ".01")
   field(SIML, "$(P)SIM")
   field(SIOL, "$(P)SIM:VOLT:SP")
   field(SDIS, "$(P)DISABLE")
   field(PREC, "2")
}

record(ai, "$(P)VOLT") {
   field(DESC, "Gets the voltage")
   field(DTYP, "stream")
   field(EGU, "V")
   field(INP, "@devWeeder.proto get_voltage($(ADDRESS)) $(PORT)")
   field(ASLO, ".01")
   field(SIML, "$(P)SIM")
   field(SIOL, "$(P)SIM:VOLT")
   field(SDIS, "$(P)DISABLE")
   field(SCAN, "1 second")
   field(PREC, "2")
}

